{
  "name": "otp-landing-page",
  "version": "1.0.0",
  "description": "OTP Landing Page with Signal Integration",
  "private": true,
  "scripts": {
    "clean": "rm -rf .output .nuxt dist",
    "build": "nuxt build",
    "start": "HOST=${HOST:-127.0.0.1} PORT=${PORT:-5500} node .output/server/index.mjs",
    "dev": "HOST=${HOST:-127.0.0.1} PORT=${PORT:-5500} nuxt dev --host ${HOST:-127.0.0.1} --port ${PORT:-5500}",
    "preview": "HOST=${HOST:-127.0.0.1} PORT=${PORT:-5500} nuxt preview",
    "start:secure": "HOST=${HOST:-127.0.0.1} PORT=${PORT:-5500} node .output/server/index.mjs",
    "start:secure:5500": "PORT=5500 HOST=${HOST:-127.0.0.1} node .output/server/index.mjs"
  },
  "keywords": [
    "otp",
    "signal",
    "landing-page"
  ],
  "author": "Signal OTP Team",
  "license": "MIT",
  "imports": {
    "#internal/nuxt/paths": "./internal/nuxt-paths.mjs"
  },
  "dependencies": {
    "nuxt": "^3.17.5"
  }
}
